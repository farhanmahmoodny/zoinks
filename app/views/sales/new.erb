<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>

    <h1>Sell Your Car</h1>

    <%= form_for @sale do |f| %>
    <%= f.label :make %>
    <%= f.text_field :make %><br><br>
    <%= f.label :model %>
    <%= f.text_field :model %><br><br>
    <%= f.label :year %>
    <%= f.number_field :year %><br><br>
    <%= f.label :color %>
    <%= f.text_field :color %><br><br>
    <%= f.label :sale_price %>
    <%= f.number_field :sale_price %><br><br>
    <%= f.label :image %>
    <%= f.text_field :image %><br><br>
    <%= f.submit "Sell" %>
    <% end %>

  </body>
</html>
